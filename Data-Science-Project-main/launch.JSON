{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome (Server)",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Dashboard File",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/dashboard.html"
        }
    ]
}
